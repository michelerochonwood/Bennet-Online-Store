    <h1>Product List</h1>
    <ul>
        {{#each products}}
            <li>
                <h2>{{this.title}}</h2>
                <p>{{this.subtitle}}</p>
                <img src="{{this.image}}" alt="{{this.title}}">
                <p>{{this.description}}</p>
                <p>${{this.price}}</p>
            </li>
        {{/each}}
    </ul>

